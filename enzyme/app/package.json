{
  "name": "enzyme-workshops",
  "version": "1.0.0",
  "main": "src/main.ts",
  "repository": "",
  "author": "Przemyslaw Beigert <przemyslaw.jan.beigert@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "yarn config:dev && webpack-dev-server --config ./webpack.config.js",
    "test": "jest src/**",
    "config:dev": "cp ./src/environments/environment.development.ts ./src/environments/environment.ts",
    "config:prod": "cp ./src/environments/environment.production.ts ./src/environments/environment.ts",
    "build:dev": "yarn config:dev && webpack --config ./webpack.config.js --mode=development",
    "build:prod": "yarn config:prod && webpack --config ./webpack.config.js --optimize-minimize --mode=production",
    "build:version": "node src/version.js",
    "build": "yarn clean && yarn build:version && yarn build:prod",
    "clean": "rm -rf dist ; rm -f src/environments/version.ts ; rm -f src/environments/version.ts"
  },
  "devDependencies": {
    "@types/enzyme": "^3.9.4",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/react-test-renderer": "^16.8.2",
    "@types/styled-components": "^4.1.16",
    "autoprefixer": "^9.6.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "file-loader": "^4.0.0",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^6.3.3",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "core-js": "^3.1.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "styled-components": "^4.3.2"
  },
  "engines": {
    "node": ">=10.9.0",
    "yarn": ">=1.16.0"
  }
}
